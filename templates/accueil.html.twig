<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        {% if is_granted('ROLE_ADMIN') %}
        <p>Bonjour Admin</p>
        
        {% elseif is_granted('ROLE_ENSEIGNANT') %}
        <p>Bonjour Enseignant</p>
        {% else  %}
        <p>Bonjour Etudiant</p>
        {% endif %}
        
    </body>
</html>